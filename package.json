{
  "name": "scoped-elements-performance",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Performance test for scoped elements",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/manolakis/scoped-elements-performance.git"
  },
  "author": "Manolakis",
  "homepage": "https://github.com/manolakis/scoped-elements-performance/",
  "scripts": {
    "start:se-mixin": "es-dev-server --root-dir test --open se-mixin/",
    "start:se-mixin-built": "es-dev-server --root-dir test --open se-mixin-built/",
    "test": "npx tachometer --config shack.json",
    "build": "babel test/se-mixin --out-dir test/se-mixin-built --copy-files",
    "test:babel-plugin": "mocha babel-plugin-scoped-elements/test/*.test.js",
    "test:babel-plugin:watch": "npm run test:babel-plugin -- --watch"
  },
  "files": [
    "*.d.ts",
    "*.js",
    "src"
  ],
  "keywords": [
    "lit-html",
    "lit-element",
    "web components",
    "performance"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@types/babel__core": "^7.1.3",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "chai": "^4.2.0",
    "chromedriver": "^79.0.0",
    "clang-format": "^1.2.4",
    "es-dev-server": "^1.40.0",
    "mocha": "^7.0.1",
    "tachometer": "^0.4.16",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "string.prototype.matchall": "^4.0.2"
  }
}
