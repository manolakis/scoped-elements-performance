{
  "name": "scoped-elements-performance",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Performance test for scoped elements",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/manolakis/scoped-elements-performance.git"
  },
  "author": "Manolakis",
  "homepage": "https://github.com/manolakis/scoped-elements-performance/",
  "scripts": {
    "start": "es-dev-server --open demo/ --watch --node-resolve",
    "test": "npx tachometer --config shack.json",
    "build": "rm -rf dist && rollup -c rollup.config.js",
    "test:babel-plugin": "mocha babel-plugin-scoped-elements/test/*.test.js",
    "test:babel-plugin:watch": "npm run test:babel-plugin -- --watch"
  },
  "files": [
    "*.d.ts",
    "*.js",
    "src"
  ],
  "keywords": [
    "lit-html",
    "lit-element",
    "web components",
    "performance"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/babel__core": "^7.1.3",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "chai": "^4.2.0",
    "chromedriver": "^79.0.0",
    "clang-format": "^1.2.4",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.40.0",
    "mocha": "^7.0.1",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.2.0",
    "string.prototype.matchall": "^4.0.2",
    "tachometer": "^0.4.16",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@lion/button": "^0.5.5",
    "@lion/checkbox": "^0.3.11",
    "@lion/checkbox-group": "^0.6.3",
    "@lion/form": "^0.4.11",
    "@lion/icon": "^0.3.3",
    "@lion/input-date": "^0.5.11",
    "@lion/input-datepicker": "^0.8.15",
    "@lion/input-range": "^0.2.11",
    "@lion/option": "^0.5.1",
    "@lion/select-rich": "^0.10.3",
    "@lion/switch": "^0.5.13",
    "@lion/tabs": "^0.2.3",
    "@lion/textarea": "^0.5.12",
    "lit-element": "^2.2.1"
  }
}
